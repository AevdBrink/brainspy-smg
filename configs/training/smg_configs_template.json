{
"results_base_dir": "tmp/output/model_nips",
"experiment_name": "main_model_nips",
"algorithm": "gradient_descent",
"hyperparameters": {
    "epochs": 80,
    "batch_size": 512,
    "optimizer":"adam",
    "learning_rate": 0.00025,
    "stop_threshold": 0.000001,
    "save_interval": 10,
    "worker_no": 0,
    "split_percentages": [0.8, 0.1, 0.1]
    },
"processor": {
    "architecture":"single_device",
    "platform": "simulation",
    "processor_type": "nn",
    "torch_model_dict": {
        "hidden_sizes": [90,90,90,90,90],
        "D_in": 7,
        "D_out": 1,
        "activation": "relu"
        }
    },
"checkpoints": {
    "use_checkpoints": true,
    "save_dir": "training_checkpoints",
    "save_interval": 20
    },
"data": {
    "training_data_path": "training_data.npz",
    "test_data_path": "tmp/output/model_nips/test_data.npz",
    "validation_size": 0.1, 
    "steps": 3
    }
}
